					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/mvarsha.666_outlook.com#ext#@mvarsha666outlook.onmicrosoft.com/spark/CurrentDate_BronzeSilverDelta.pySpark"
				},
